.full-screen {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
}

.center-content {
    justify-content: center;
    align-content: center;
    align-items: center;
}

.background-dark {
    background-color: rgb(51, 51, 51);
}

.background-light {
    background-color: rgb(102, 102, 102);
}
